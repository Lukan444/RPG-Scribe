{
  "compilerOptions": {
    "target": "es2015",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "downlevelIteration": true,
    "baseUrl": ".",
    "paths": {
      "reactflow": ["node_modules/reactflow"],
      "reactflow/*": ["node_modules/reactflow/*"],
      "@reactflow/*": ["node_modules/@reactflow/*"],
      "@hello-pangea/dnd": ["src/types/hello-pangea__dnd.d.ts"]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ],
  "files": [
    "src/tests/vitest/testing-library-jest-dom.d.ts"
  ]
}
